{
   "files.exclude": {
      ".cache": true,
      "icon": true,
      "node_modules": true,
      "public": true,
      ".env": true,
      ".gitignore": true,
      "desktop.ini": true,
      "gatsby-node.js": true,
      // "gatsby-config.js": true,
      "package-lock.json": true,
      "package.json": true,
      "jsconfig.json": true
   },

   "files.associations": {
      "*.js": "javascriptreact"
   },

   "vsicons.associations.folders": [
      {
         "icon": "config",
         "extensions": ["react"]
      },
      {
         "icon": "component",
         "extensions": ["Component-groups"]
      }
   ]
}
